CREATE TABLE Student (
    Id INT PRIMARY KEY,
    Name NVARCHAR(100), -- Assuming names can be up to 100 characters long
    Branch VARCHAR(50), -- Branch name, assumed to be a shorter string
    Marks DECIMAL(5, 2), -- Marks with a precision of 5 digits and 2 decimal places
    Location NVARCHAR(100) -- Location where the student resides
);
